from typing import Tuple, Set, Iterable, List


class DateTimeSerializationMode:
    Default = 0
    Roundtrip = 1
    Local = 2


class DateTimeSerializationSection(ConfigurationSection):
    def __init__(self): ...
    @property
    def Mode(self) -> DateTimeSerializationMode: ...
    @Mode.setter
    def Mode(self, value: DateTimeSerializationMode) -> None: ...


class RootedPathValidator(ConfigurationValidatorBase):
    def __init__(self): ...
    def CanValidate(self, type: Type) -> bool: ...
    def Validate(self, value: Object) -> None: ...


class SchemaImporterExtensionElement(ConfigurationElement):
    @overload
    def __init__(self): ...
    @overload
    def __init__(self, name: str, type: str): ...
    @overload
    def __init__(self, name: str, type: Type): ...
    @property
    def Name(self) -> str: ...
    @property
    def Type(self) -> Type: ...
    @Name.setter
    def Name(self, value: str) -> None: ...
    @Type.setter
    def Type(self, value: Type) -> None: ...


class SchemaImporterExtensionElementCollection(ConfigurationElementCollection):
    def __init__(self): ...
    def Add(self, element: SchemaImporterExtensionElement) -> None: ...
    def Clear(self) -> None: ...
    @property
    def Item(self, index: int) -> SchemaImporterExtensionElement: ...
    @property
    def Item(self, name: str) -> SchemaImporterExtensionElement: ...
    def IndexOf(self, element: SchemaImporterExtensionElement) -> int: ...
    @overload
    def Remove(self, name: str) -> None: ...
    @overload
    def Remove(self, element: SchemaImporterExtensionElement) -> None: ...
    def RemoveAt(self, index: int) -> None: ...
    @Item.setter
    def Item(self, index: int, value: SchemaImporterExtensionElement) -> None: ...
    @Item.setter
    def Item(self, name: str, value: SchemaImporterExtensionElement) -> None: ...


class SchemaImporterExtensionsSection(ConfigurationSection):
    def __init__(self): ...
    @property
    def SchemaImporterExtensions(self) -> SchemaImporterExtensionElementCollection: ...


class SerializationSectionGroup(ConfigurationSectionGroup):
    def __init__(self): ...
    @property
    def DateTimeSerialization(self) -> DateTimeSerializationSection: ...
    @property
    def SchemaImporterExtensions(self) -> SchemaImporterExtensionsSection: ...
    @property
    def XmlSerializer(self) -> XmlSerializerSection: ...


class XmlSerializerSection(ConfigurationSection):
    def __init__(self): ...
    @property
    def CheckDeserializeAdvances(self) -> bool: ...
    @property
    def TempFilesLocation(self) -> str: ...
    @property
    def UseLegacySerializerGeneration(self) -> bool: ...
    @CheckDeserializeAdvances.setter
    def CheckDeserializeAdvances(self, value: bool) -> None: ...
    @TempFilesLocation.setter
    def TempFilesLocation(self, value: str) -> None: ...
    @UseLegacySerializerGeneration.setter
    def UseLegacySerializerGeneration(self, value: bool) -> None: ...
