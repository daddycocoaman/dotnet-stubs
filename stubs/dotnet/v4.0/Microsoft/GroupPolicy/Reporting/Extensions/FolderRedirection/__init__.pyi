from typing import Tuple, Set, Iterable, List


class ConfigurationControlType:
    Wmi = 0
    GP = 1
    NA = 2
    NR = 3


class FolderRedirectionSettings(GPOExtension):
    def __init__(self): ...
    @property
    def Folder(self) -> Set(FolderSetting): ...
    def Initialize(self, context: ReportingContext) -> None: ...
    @Folder.setter
    def Folder(self, value: Set(FolderSetting)) -> None: ...


class FolderSetting(PolicySetting):
    @overload
    def __init__(self): ...
    @overload
    def __init__(self, frSetting: FRFolderSetting, server: str): ...
    @overload
    def __init__(self, folderObject: ManagementObject, server: str): ...
    @property
    def ApplyToDownLevel(self) -> bool: ...
    @property
    def ConfigurationControl(self) -> ConfigurationControlType: ...
    @property
    def DoNotCare(self) -> bool: ...
    @property
    def FollowParent(self) -> bool: ...
    @property
    def GrantExclusiveRights(self) -> bool: ...
    @property
    def Id(self) -> str: ...
    @property
    def Location(self) -> Set(FolderSettingLocation): ...
    @property
    def MoveContents(self) -> bool: ...
    @property
    def PolicyRemovalBehavior(self) -> RemovalBehavior: ...
    @property
    def PrimaryComputerEvaluation(self) -> PrimaryComputerEvaluationType: ...
    @property
    def RedirectToLocal(self) -> bool: ...
    @ApplyToDownLevel.setter
    def ApplyToDownLevel(self, value: bool) -> None: ...
    @ConfigurationControl.setter
    def ConfigurationControl(self, value: ConfigurationControlType) -> None: ...
    @DoNotCare.setter
    def DoNotCare(self, value: bool) -> None: ...
    @FollowParent.setter
    def FollowParent(self, value: bool) -> None: ...
    @GrantExclusiveRights.setter
    def GrantExclusiveRights(self, value: bool) -> None: ...
    @Id.setter
    def Id(self, value: str) -> None: ...
    @Location.setter
    def Location(self, value: Set(FolderSettingLocation)) -> None: ...
    @MoveContents.setter
    def MoveContents(self, value: bool) -> None: ...
    @PolicyRemovalBehavior.setter
    def PolicyRemovalBehavior(self, value: RemovalBehavior) -> None: ...
    @PrimaryComputerEvaluation.setter
    def PrimaryComputerEvaluation(self, value: PrimaryComputerEvaluationType) -> None: ...
    @RedirectToLocal.setter
    def RedirectToLocal(self, value: bool) -> None: ...


class FolderSettingLocation:
    @overload
    def __init__(self): ...
    @overload
    def __init__(self, groupSid: str, path: str, server: str): ...
    @property
    def DestinationPath(self) -> str: ...
    @property
    def SecurityGroup(self) -> Trustee: ...
    @DestinationPath.setter
    def DestinationPath(self, value: str) -> None: ...
    @SecurityGroup.setter
    def SecurityGroup(self, value: Trustee) -> None: ...


class PrimaryComputerEvaluationType:
    Invalid = 0
    NotPrimaryComputer = 1
    IsPrimaryComputer = 2
    PrimaryComputerPolicyDisabled = 3
    PrimaryComputerRoamingProfileOverride = 4
    PrimaryComputerPolicyNotApplicable = 5
    PrimaryComputerPolicyNotReachable = 6


class RemovalBehavior:
    LeaveContents = 0
    RestoreContents = 1
