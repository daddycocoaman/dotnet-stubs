from typing import Tuple, Set, Iterable, List


class ApplicableBinaries:
    #None = 0
    NonLibraries = 1
    All = 2


class ApplicableUsers:
    All = 0
    NonAdmins = 1


class CertificateRule(Rule_at):
    def __init__(self): ...
    @property
    def Certificate(self) -> Set(Byte): ...
    @property
    def Description(self) -> str: ...
    @property
    def SubjectName(self) -> str: ...
    @Certificate.setter
    def Certificate(self, value: Set(Byte)) -> None: ...
    @Description.setter
    def Description(self, value: str) -> None: ...
    @SubjectName.setter
    def SubjectName(self, value: str) -> None: ...


class CertificateRulesEnabled:
    IgnoreCertificateRules = 0
    EnforceCertificateRules = 1


class HashRule(Rule_at):
    def __init__(self): ...
    @property
    def Description(self) -> str: ...
    @property
    def FileDescription(self) -> str: ...
    @property
    def FileSize(self) -> UInt64: ...
    @Description.setter
    def Description(self, value: str) -> None: ...
    @FileDescription.setter
    def FileDescription(self, value: str) -> None: ...
    @FileSize.setter
    def FileSize(self, value: UInt64) -> None: ...


class InternetZoneRule(Rule_at):
    def __init__(self): ...
    @property
    def Zone(self) -> Zone: ...
    @Zone.setter
    def Zone(self, value: Zone) -> None: ...


class PathRule(Rule_at):
    def __init__(self): ...
    @property
    def Description(self) -> str: ...
    @property
    def Path(self) -> str: ...
    @Description.setter
    def Description(self, value: str) -> None: ...
    @Path.setter
    def Path(self, value: str) -> None: ...


class Rule_at(PolicySetting):
    @property
    def ModificationTime(self) -> DateTime: ...
    @property
    def SecurityLevel(self) -> SecurityLevel: ...
    @ModificationTime.setter
    def ModificationTime(self, value: DateTime) -> None: ...
    @SecurityLevel.setter
    def SecurityLevel(self, value: SecurityLevel) -> None: ...


class SecurityLevel:
    Disallowed = 0
    Untrusted = 4096
    Restricted = 65536
    BasicUser = 131072
    Unrestricted = 262144


class SoftwareRestrictionGeneralProperties(PolicySetting):
    def __init__(self): ...
    @property
    def ApplicableBinaries(self) -> ApplicableBinaries: ...
    @property
    def ApplicableUsers(self) -> ApplicableUsers: ...
    @property
    def CertificateRulesEnabled(self) -> CertificateRulesEnabled: ...
    @property
    def DefaultSecurityLevel(self) -> SecurityLevel: ...
    @property
    def ExecutableFiles(self) -> Set(str): ...
    @property
    def TrustedPublishers(self) -> TrustedPublishers: ...
    @ApplicableBinaries.setter
    def ApplicableBinaries(self, value: ApplicableBinaries) -> None: ...
    @ApplicableUsers.setter
    def ApplicableUsers(self, value: ApplicableUsers) -> None: ...
    @CertificateRulesEnabled.setter
    def CertificateRulesEnabled(self, value: CertificateRulesEnabled) -> None: ...
    @DefaultSecurityLevel.setter
    def DefaultSecurityLevel(self, value: SecurityLevel) -> None: ...
    @ExecutableFiles.setter
    def ExecutableFiles(self, value: Set(str)) -> None: ...
    @TrustedPublishers.setter
    def TrustedPublishers(self, value: TrustedPublishers) -> None: ...


class SoftwareRestrictionSettings(GPOExtension):
    def __init__(self): ...
    @property
    def General(self) -> SoftwareRestrictionGeneralProperties: ...
    @property
    def Items(self) -> ArrayList: ...
    def Initialize(self, context: ReportingContext) -> None: ...
    @General.setter
    def General(self, value: SoftwareRestrictionGeneralProperties) -> None: ...
    @Items.setter
    def Items(self, value: ArrayList) -> None: ...


class TrustedPublishers:
    def __init__(self): ...
    @property
    def PublishSelector(self) -> TrustedPublishersPublishSelector: ...
    @property
    def RevokeCheck(self) -> TrustedPublishersRevokeCheck: ...
    @PublishSelector.setter
    def PublishSelector(self, value: TrustedPublishersPublishSelector) -> None: ...
    @RevokeCheck.setter
    def RevokeCheck(self, value: TrustedPublishersRevokeCheck) -> None: ...


class TrustedPublishersPublishSelector:
    Users = 0
    LocalAdmins = 1
    EnterpriseAdmins = 2


class TrustedPublishersRevokeCheck:
    def __init__(self): ...
    @property
    def Publisher(self) -> bool: ...
    @property
    def Timestamp(self) -> bool: ...
    @Publisher.setter
    def Publisher(self, value: bool) -> None: ...
    @Timestamp.setter
    def Timestamp(self, value: bool) -> None: ...


class Zone:
    LocalComputer = 0
    LocalIntranet = 1
    TrustedSites = 2
    Internet = 3
    RestrictedSites = 4
