from typing import Tuple, Set, Iterable, List


class OpenApiRouteHandlerBuilderExtensions:
    @overload
    def Accepts(builder: RouteHandlerBuilder, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    @overload
    def Accepts(builder: RouteHandlerBuilder, isOptional: bool, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    @overload
    def Accepts(builder: RouteHandlerBuilder, requestType: Type, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    @overload
    def Accepts(builder: RouteHandlerBuilder, requestType: Type, isOptional: bool, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    def ExcludeFromDescription(builder: RouteHandlerBuilder) -> RouteHandlerBuilder: ...
    @overload
    def Produces(builder: RouteHandlerBuilder, statusCode: int, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    @overload
    def Produces(builder: RouteHandlerBuilder, statusCode: int, responseType: Type, contentType: str, additionalContentTypes: Set(str)) -> RouteHandlerBuilder: ...
    def ProducesProblem(builder: RouteHandlerBuilder, statusCode: int, contentType: str) -> RouteHandlerBuilder: ...
    def ProducesValidationProblem(builder: RouteHandlerBuilder, statusCode: int, contentType: str) -> RouteHandlerBuilder: ...
    def WithTags(builder: RouteHandlerBuilder, tags: Set(str)) -> RouteHandlerBuilder: ...
