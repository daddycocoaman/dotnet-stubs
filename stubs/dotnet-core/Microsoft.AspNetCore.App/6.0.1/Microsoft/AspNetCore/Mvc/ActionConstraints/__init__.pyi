from typing import Tuple, Set, Iterable, List


class ActionMethodSelectorAttribute:
    def Accept(self, context: ActionConstraintContext) -> bool: ...
    @property
    def Order(self) -> int: ...
    def IsValidForRequest(self, routeContext: RouteContext, action: ActionDescriptor) -> bool: ...
    @Order.setter
    def Order(self, value: int) -> None: ...


class HttpMethodActionConstraint:
    def __init__(self, httpMethods: Iterable[str]): ...
    def Accept(self, context: ActionConstraintContext) -> bool: ...
    @property
    def HttpMethods(self) -> Iterable[str]: ...
    @property
    def Order(self) -> int: ...
