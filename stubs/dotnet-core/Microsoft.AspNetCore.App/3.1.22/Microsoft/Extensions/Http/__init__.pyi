__all__ = ['Logging']
from typing import Tuple, Set, Iterable, List


class HttpClientFactoryOptions:
    def __init__(self): ...
    @property
    def HandlerLifetime(self) -> TimeSpan: ...
    @property
    def HttpClientActions(self) -> List[HttpClient]: ...
    @property
    def HttpMessageHandlerBuilderActions(self) -> List[HttpMessageHandlerBuilder]: ...
    @property
    def SuppressHandlerScope(self) -> bool: ...
    @HandlerLifetime.setter
    def HandlerLifetime(self, value: TimeSpan) -> None: ...
    @SuppressHandlerScope.setter
    def SuppressHandlerScope(self, value: bool) -> None: ...


class HttpMessageHandlerBuilder:
    def Build(self) -> HttpMessageHandler: ...
    @property
    def AdditionalHandlers(self) -> List[DelegatingHandler]: ...
    @property
    def Name(self) -> str: ...
    @property
    def PrimaryHandler(self) -> HttpMessageHandler: ...
    @property
    def Services(self) -> IServiceProvider: ...
    @Name.setter
    def Name(self, value: str) -> None: ...
    @PrimaryHandler.setter
    def PrimaryHandler(self, value: HttpMessageHandler) -> None: ...


class IHttpMessageHandlerBuilderFilter:
    def Configure(self, next: Action) -> Action: ...


