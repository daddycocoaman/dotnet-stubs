from typing import Tuple, Set, Iterable, List


class XmlConfigurationProvider(FileConfigurationProvider):
    def __init__(self, source: XmlConfigurationSource): ...
    @overload
    def Load(self, stream: Stream) -> None: ...


class XmlConfigurationSource(FileConfigurationSource):
    def __init__(self): ...
    def Build(self, builder: IConfigurationBuilder) -> IConfigurationProvider: ...


class XmlDocumentDecryptor:
    def CreateDecryptingXmlReader(self, input: Stream, settings: XmlReaderSettings) -> XmlReader: ...


class XmlStreamConfigurationProvider(StreamConfigurationProvider):
    def __init__(self, source: XmlStreamConfigurationSource): ...
    @overload
    def Load(self, stream: Stream) -> None: ...
    def Read(stream: Stream, decryptor: XmlDocumentDecryptor) -> IDictionary: ...


class XmlStreamConfigurationSource(StreamConfigurationSource):
    def __init__(self): ...
    def Build(self, builder: IConfigurationBuilder) -> IConfigurationProvider: ...
