from typing import Tuple, Set, Iterable, List


class IViewBufferScope:
    def CreateWriter(self, writer: TextWriter) -> TextWriter: ...
    def GetPage(self, pageSize: int) -> Set(ViewBufferValue): ...
    def ReturnSegment(self, segment: Set(ViewBufferValue)) -> None: ...


class ViewBufferValue:
    @overload
    def __init__(self, value: str): ...
    @overload
    def __init__(self, content: IHtmlContent): ...
    @property
    def Value(self) -> Object: ...
