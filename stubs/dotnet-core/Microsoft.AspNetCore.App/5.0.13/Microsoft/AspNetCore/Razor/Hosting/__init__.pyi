from typing import Tuple, Set, Iterable, List


class IRazorSourceChecksumMetadata:
    @property
    def Checksum(self) -> str: ...
    @property
    def ChecksumAlgorithm(self) -> str: ...
    @property
    def Identifier(self) -> str: ...


class RazorCompiledItem:
    @property
    def Identifier(self) -> str: ...
    @property
    def Kind(self) -> str: ...
    @property
    def Metadata(self) -> IReadOnlyList: ...
    @property
    def Type(self) -> Type: ...


class RazorCompiledItemAttribute:
    def __init__(self, type: Type, kind: str, identifier: str): ...
    @property
    def Identifier(self) -> str: ...
    @property
    def Kind(self) -> str: ...
    @property
    def Type(self) -> Type: ...


