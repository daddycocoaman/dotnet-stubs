from typing import Tuple, Set, Iterable, List


class HstsMiddleware:
    @overload
    def __init__(self, next: RequestDelegate, options: IOptions): ...
    @overload
    def __init__(self, next: RequestDelegate, options: IOptions, loggerFactory: ILoggerFactory): ...
    def Invoke(self, context: HttpContext) -> Task: ...


class HstsOptions:
    def __init__(self): ...
    @property
    def ExcludedHosts(self) -> List[str]: ...
    @property
    def IncludeSubDomains(self) -> bool: ...
    @property
    def MaxAge(self) -> TimeSpan: ...
    @property
    def Preload(self) -> bool: ...
    @IncludeSubDomains.setter
    def IncludeSubDomains(self, value: bool) -> None: ...
    @MaxAge.setter
    def MaxAge(self, value: TimeSpan) -> None: ...
    @Preload.setter
    def Preload(self, value: bool) -> None: ...


class HttpsRedirectionMiddleware:
    @overload
    def __init__(self, next: RequestDelegate, options: IOptions, config: IConfiguration, loggerFactory: ILoggerFactory): ...
    @overload
    def __init__(self, next: RequestDelegate, options: IOptions, config: IConfiguration, loggerFactory: ILoggerFactory, serverAddressesFeature: IServerAddressesFeature): ...
    def Invoke(self, context: HttpContext) -> Task: ...


class HttpsRedirectionOptions:
    def __init__(self): ...
    @property
    def HttpsPort(self) -> Nullable: ...
    @property
    def RedirectStatusCode(self) -> int: ...
    @HttpsPort.setter
    def HttpsPort(self, value: Nullable) -> None: ...
    @RedirectStatusCode.setter
    def RedirectStatusCode(self, value: int) -> None: ...
