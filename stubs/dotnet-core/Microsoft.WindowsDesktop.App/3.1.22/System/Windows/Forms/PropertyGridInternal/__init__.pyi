from typing import Tuple, Set, Iterable, List


class IRootGridEntry:
    @property
    def BrowsableAttributes(self) -> AttributeCollection: ...
    def ResetBrowsableAttributes(self) -> None: ...
    @BrowsableAttributes.setter
    def BrowsableAttributes(self, value: AttributeCollection) -> None: ...
    def ShowCategories(self, showCategories: bool) -> None: ...


class PropertiesTab(PropertyTab):
    def __init__(self): ...
    @property
    def HelpKeyword(self) -> str: ...
    @property
    def TabName(self) -> str: ...
    def GetDefaultProperty(self, obj: Object) -> PropertyDescriptor: ...
    @overload
    def GetProperties(self, component: Object, attributes: Set(Attribute)) -> PropertyDescriptorCollection: ...
    @overload
    def GetProperties(self, context: ITypeDescriptorContext, component: Object, attributes: Set(Attribute)) -> PropertyDescriptorCollection: ...


class PropertyGridCommands(Object):
    def __init__(self): ...
