from typing import Tuple, Set, Iterable, List


class SqlClientPermission(DBDataPermission):
    @overload
    def __init__(self): ...
    @overload
    def __init__(self, state: PermissionState): ...
    @overload
    def __init__(self, state: PermissionState, allowBlankPassword: bool): ...
    def Add(self, connectionString: str, restrictions: str, behavior: KeyRestrictionBehavior) -> None: ...
    def Copy(self) -> IPermission: ...


class SqlClientPermissionAttribute(DBDataPermissionAttribute):
    def __init__(self, action: SecurityAction): ...
    def CreatePermission(self) -> IPermission: ...
