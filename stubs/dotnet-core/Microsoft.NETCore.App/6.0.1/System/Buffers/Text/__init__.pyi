from typing import Tuple, Set, Iterable, List


class Base64(Object):
    def DecodeFromUtf8(utf8: ReadOnlySpan, bytes: Span, isFinalBlock: bool) -> Tuple[OperationStatus, int, int]: ...
    def DecodeFromUtf8InPlace(buffer: Span) -> Tuple[OperationStatus, int]: ...
    def EncodeToUtf8(bytes: ReadOnlySpan, utf8: Span, isFinalBlock: bool) -> Tuple[OperationStatus, int, int]: ...
    def EncodeToUtf8InPlace(buffer: Span, dataLength: int) -> Tuple[OperationStatus, int]: ...
    def GetMaxDecodedFromUtf8Length(length: int) -> int: ...
    def GetMaxEncodedToUtf8Length(length: int) -> int: ...
